//SQL queries used for this project. 
//Note this only works in mysql for orcale auto_increment is not there we need to create a sequence and assign seperately check master for those steps


create schema hibernateonetoone

create table instructor_detail (id int not null auto_increment primary key, youtube_channel varchar(40) default null, hobby varchar(20) default null);  

create table instructor (id int not null auto_increment primary key, first_name varchar(20), email varchar(20), instructor_id int default null,constraint crfk_key foreign key (instructor_id) references instructor_detail(id) ON delete no action ON update no action);

select * from course;

select * from Course_instructor;

select * from Course_instructor_detail;